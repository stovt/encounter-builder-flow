{
  "name": "encounter-builder",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://sergeytovt.github.io/encounter-builder",
  "dependencies": {
    "connected-react-router": "^6.2.2",
    "dotenv": "^7.0.0",
    "history": "^4.7.2",
    "node-fetch": "^2.3.0",
    "polished": "^3.0.0",
    "react": "^16.7.0",
    "react-calculator-input": "^1.1.1",
    "react-dom": "^16.7.0",
    "react-intl": "^2.8.0",
    "react-portal-tooltip": "^2.4.6",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "react-select": "^2.4.1",
    "react-styled-flexboxgrid": "^3.0.0",
    "react-table": "^6.9.2",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-saga": "^1.0.1",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "postinstall": "flow-typed install | flow-typed create-stub polished | flow-typed install jest@23",
    "start": "react-scripts start",
    "prebuild": "rimraf build/",
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "run-p lint:js lint:styles",
    "lint:js": "eslint src/",
    "lint:styles": "stylelint src/",
    "lint:fix": "yarn lint:js --fix",
    "flow:start": "flow",
    "flow:check": "flow check",
    "flow-typed:update": "flow-typed update-cache",
    "analyze": "source-map-explorer build/static/js/main.*",
    "parse-monsters": "node scripts/parseMonsters.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint flow:check"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.94.0",
    "flow-typed": "^2.5.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "source-map-explorer": "^1.6.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
